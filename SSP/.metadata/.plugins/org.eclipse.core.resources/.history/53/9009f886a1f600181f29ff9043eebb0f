package ex0306.Sub;//右クリ→パッケージで作成したパスが自動的に追加される(Eclipseの機能

public class User {
    private String userId;
    private String password;

    //コンストラクタ(初期設定用の
    public User(String userId, String password) {
        this.userId = userId;
        this.password = password;
    }
    String getUserId() {
        return this.userId;
    }
    String getPassword() {
        return this.password;
    }
}
