package kadai0601;

public class Calculate {
	private int price;
	private String msg;

	public int getPrice() {
		return this.price;
	}

	public void setPrice(int price) {
		this.price = price;
	}

	public void setErrorMsg(String msg) {
		this.msg = msg;
	}

	public String getErrorMsg() {
		return this.msg;
	}

}
